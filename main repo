repeat task.wait() until game:IsLoaded()
repeat task.wait() until game.PlaceId
if game.PlaceId ~= 16498369169 then
    while true do task.wait()
        game:GetService("TeleportService"):Teleport(16498369169, game.Players.LocalPlayer)
    end
end
repeat task.wait() until workspace
repeat task.wait() until workspace:FindFirstChild("Map2"); repeat task.wait() until workspace.Map2:FindFirstChild("100 | Tech Spawn")
repeat task.wait() until workspace.Map2["100 | Tech Spawn"]:FindFirstChild("INTERACT") and workspace.Map2["100 | Tech Spawn"].INTERACT:FindFirstChild("Machines")
repeat task.wait() until workspace.Map2["100 | Tech Spawn"].INTERACT.Machines:FindFirstChild("SpinnyWheel") and workspace.Map2["100 | Tech Spawn"].INTERACT.Machines.SpinnyWheel:FindFirstChild("SpinnyWheel")
repeat task.wait() until workspace.Map2["100 | Tech Spawn"].INTERACT.Machines.SpinnyWheel.SpinnyWheel.SurfaceGui:FindFirstChild("Wheel") and workspace.Map2["100 | Tech Spawn"].INTERACT.Machines.SpinnyWheel.SpinnyWheel.SurfaceGui.Wheel:FindFirstChild("WheelCore")
repeat task.wait() until workspace.Map2["100 | Tech Spawn"].INTERACT.Machines.SpinnyWheel.SpinnyWheel.SurfaceGui.Wheel.WheelCore:FindFirstChild("Prize1") and workspace.Map2["100 | Tech Spawn"].INTERACT.Machines.SpinnyWheel.SpinnyWheel.SurfaceGui.Wheel.WheelCore.Prize1:FindFirstChild("PrizeRender")
print("Loaded in")

function ServerHop()
    while task.wait() do
        pcall(function()
            game:GetService("TeleportService"):TeleportToPlaceInstance(game.PlaceId, game.JobId, game.Players.LocalPlayer)
        end)
        task.wait(1)
    end
end

SpinnyWheelCore = workspace.Map2["100 | Tech Spawn"].INTERACT.Machines.SpinnyWheel.SpinnyWheel.SurfaceGui.Wheel.WheelCore
for i, v in pairs(SpinnyWheelCore:GetChildren()) do
    if v.Name:match("Prize") then
        if v.PrizeRender.Icon.Image == "rbxassetid://16471981261" or v.PrizeRender.Icon.Image == "rbxassetid://16471981376" then
            print("Found a Huge in Wheel!")
            a,b = game:GetService("ReplicatedStorage").Network["Spinny Wheel: Request Spin"]:InvokeServer("TechWheel")
            task.wait(3)
        end
    end
end
ServerHop()
